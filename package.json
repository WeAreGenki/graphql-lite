{
  "name": "graphql-lite",
  "version": "0.0.0",
  "description": "üçÉ Lightweight GraphQL client",
  "repository": "https://github.com/WeAreGenki/graphql-lite",
  "bugs": "https://github.com/WeAreGenki/graphql-lite/issues",
  "homepage": "https://wearegenki.github.io/graphql-lite",
  "author": "Max Milton <max@wearegenki.com>",
  "license": "Apache-2.0",
  "keywords": [
    "graphql-client",
    "graphql",
    "typescript",
    "wearegenki"
  ],
  "main": "dist/index.js",
  "scripts": {
    "dev": "rollup --config --watch",
    "build": "rollup --config",
    "lint": "yarn lint:ts && yarn lint:types",
    "lint:ts": "tslint --project tsconfig.json || true",
    "lint:types": "tsc --noEmit",
    "pretest": "yarn lint",
    "test": "JEST_CIRCUS=1 jest --notify --watch",
    "test-ci": "JEST_CIRCUS=1 jest --coverage --runInBand --ci"
  },
  "peerDependencies": {
    "graphql": "^14.0.0"
  },
  "dependencies": {},
  "devDependencies": {
    "@minna-ui/jest-config": "0.21.0",
    "@minna-ui/ts-config": "0.21.0",
    "@minna-ui/tslint-config": "0.21.0",
    "@wearegenki/renovate-config": "1.0.0",
    "codecov": "3.2.0",
    "jest": "24.1.0",
    "prettier": "1.16.4",
    "rollup": "1.1.2",
    "rollup-plugin-typescript": "1.0.0",
    "ts-jest": "24.0.0",
    "tslib": "1.9.3",
    "tslint": "5.12.1",
    "typescript": "3.3.3"
  },
  "jest": {
    "preset": "@minna-ui/jest-config"
  },
  "prettier": {
    "arrowParens": "always",
    "endOfLine": "lf",
    "singleQuote": true,
    "trailingComma": "all"
  },
  "renovate": {
    "extends": [
      "@wearegenki"
    ]
  },
  "publishConfig": {
    "access": "public"
  }
}
